<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="LoginSIME.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:LoginSIME"
    xmlns:views="clr-namespace:LoginSIME.Views"
    Shell.BackgroundColor="#44A1A0"
    Shell.FlyoutBehavior="Disabled"
    Title="LoginSIME" >

    <ShellContent
        ContentTemplate="{DataTemplate views:LoginView}"
        Route="MainPage" />

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate views:HomeView}"
        Route="Home" />

</Shell>
